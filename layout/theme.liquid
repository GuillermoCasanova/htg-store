      /*------------------------------------*\
          #Typography
      \*------------------------------------*/

      @font-face {
        font-family: 'NimbusSansDOT-ReguCond';
        src: url({{ 'NimbusSansDOT-ReguCond.otf' | asset_url }}) format('opentype');
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }


      @font-face {
        font-family: 'NimbusSansDOT-ReguCondItal';
        src: url({{ 'NimbusSansDOT-ReguCondItal.otf' | asset_url }}) format('opentype');
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }


      @font-face {
        font-family: 'Caxton_Light';
        src: url({{ 'Caxton_Light.otf' | asset_url }}) format('opentype');
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }


      :root {
        --primary-font-family: 'NimbusSansDOT-ReguCond', 'Helvetica', 'sans-serif';
        --secondary-font-family: 'NimbusSansDOT-ReguCondItal', 'Helvetica', 'sans-serif';
        --tertiary-font-family: 'Caxton_Light', 'Times', 'Times New Roman', serif;
        --letter-spacing-1: 1.5px;
        --letter-spacing-2: 1px;
        --letter-spacing-3: 3px;
        --letter-spacing-4: 5px;
        --kilo: 3.815rem;
        --h1: 3.052rem;
        --h2: 2.441rem;
        --h3: 1.953rem;
        --h4: 1.563rem;
        --h5: 1.25rem;
        --h6: 1.25rem;
        --normal: 1.15rem;
        --micro: 1rem;
        --mini: .9rem;
        --line-height-normal: 1.25;
        --line-height-double: 1.5;
      }
