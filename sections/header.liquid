{% style %}
/*------------------------------------*\
    #Section Header
\*------------------------------------*/


/* section-header */
#shopify-section-header {
    z-index: 3;
}

.shopify-section-header-sticky {
    position: sticky;
    top: 0;
}

.shopify-section-header-hidden {
    transform: translateY(-100%);
}

.shopify-section-header-hidden.menu-open {
    transform: translateY(0);
}

#shopify-section-header.animate {
    transition: transform 0.15s ease-out;
}

.header-wrapper {
    display: block;
    position: relative;
}


/*------------------------------------*\
    #Header
\*------------------------------------*/

sticky-header {
    z-index: 20;
    top: 0;
}


@media only screen and (min-width: 930px) {
    sticky-header {
        z-index: 2;
    }
}

.header-wrapper {
    width: 100%;
}


.header {
    transition: all .6s cubic-bezier(0.165, 0.84, 0.44, 1);
    position: relative;
    border-bottom: 1px solid rgb(var(--color-base-text));
}

.header__inner {
    padding-top: var(--level1);
    padding-bottom: var(--level1);
    display: grid;
    grid-template-areas: 'menu  heading cart-drawer';
    grid-template-columns: 1fr 2fr 1fr;
    justify-content: space-between;
    align-items: center;
}

@media screen and (min-width: 930px) {
    .header__inner {
        grid-template-columns: 2fr 1fr 2fr;
        padding-top: var(--level3);
        padding-bottom: var(--level3);
    }
}

.header header-drawer {
    justify-self: start;
}

.header *[tabindex='-1']:focus {
    outline: none;
}



/*------------------------------------*\
    #Header Heading
\*------------------------------------*/


.header__heading {
    margin: 0;
    line-height: 0;
    grid-area: heading;
    justify-self: center;
    z-index: 2;
}

.header__heading-link {
    display: inline-block;
    text-decoration: none;
    word-break: break-word;
}

.header__heading-logo {
    height: auto;
    width: 100%;
}

.header__logo {
    width: 100%;
    height: 2rem;
    max-width: 10rem;
    margin-right: auto;
    margin-left: auto;
    color: var(--color-base-text);
    fill: var(--color-base-text);
    transition: all var(--duration-default) ease 0s;
    z-index: 0;
}

@media screen and (min-width: 930px) {
    .header__logo {
        max-width: 13.5rem;
    }
}





/*------------------------------------*\
    #Header Drawer Blocker
\*------------------------------------*/

.header-drawer-blocker {
    width: 100%;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    height: 100%;
    background: rgb(var(--color-base-background-1));
    z-index: 1;
}

@media screen and (min-width: 930px) {
    header-drawer {
        display: none;
    }
}





/*------------------------------------*\
    #Header Icons
\*------------------------------------*/

.header__icons {
    position: relative;
}


@media screen and (min-width: 930px) {
    .header__icons {
        top: 0;
    }
}


@media screen and (max-width: 989px) {
    menu-drawer~.header__icons .header__icon--account {
        display: none;
    }
}


.header__icon:not(.header__icon--summary),
.header__icon span {
    display: flex;
    align-items: center;
    justify-content: center;
}

.header__icon span {
    height: 100%;
}

.header__icon:hover {
    cursor: pointer;
}

.header__icon .icon {
    fill: var(--color-base-text);
    vertical-align: middle;
}

.header__icon svg {
    width: 100%;
    height: 100%;
}





/*------------------------------------*\
    #Header Icons - Cart Toggle
\*------------------------------------*/

cart-toggle {
    grid-area: cart-drawer;
    display: flex;
    justify-content: flex-end;
    flex-direction: row;
}

cart-toggle.hide-for-large-up {
    display: Flex;
}

@media only screen and (min-width: 930px) {
    cart-toggle.hide-for-large-up {
        display: none;
    }
}


.header__icon--cart {
    width: 4rem;
    height: 3rem;
    display: block;
    top: -.15rem;
}


.header__icon svg {
    width: 100%;
    height: 100%;
}

.header.is-product-page .header__icon--cart {
    color: black;
}

.header.menu-is-open .header__icon--cart {
    color: black;
}

.header__icon--cart {
    z-index: 1;
    position: relative;
    color: black;
    background-color: transparent;
    position: relative;
    border: 0;
    padding: 0;
    transition: all var(--duration-default) ease-in-out;
    margin-right: calc(-1 * var(--level3));
}


.header__icon--cart--text {
    width: auto;
    height: auto;
    top: 0;
    padding: var(--level3);
    text-decoration: none;
    list-style: none;
    color: rgb(var(--color-base-text));
    transition: all var(--duration-default) ease 0s;
    background: transparent;
    border: none;
    text-transform: uppercase;
    line-height: 1;
    padding-bottom: var(--level1);
    font-size: var(--h4);
    display: inline-block;
}


.header__icon--cart--text:hover,
.header__icon--cart--text:focus {
    text-decoration: underline;
}


/* 
.header__icon--cart.is-open {
    opacity: .5;
}

@media screen and (min-width: 930px) {
    .header__icon--cart.is-open {
        opacity: 1;
    }
} */


.header__icon--cart .icon {
    width: 2rem;
    height: 1.4rem;
}



/* component-cart-count-bubble */
.cart-count-bubble:empty {
    display: none;
}

.cart-count-bubble {
    position: absolute;
    margin: auto;
    height: 1rem;
    width: 1rem;
    bottom: 15%;
    right: 25%;
    padding-top: 1px;
    margin: auto;
    border-radius: 100%;
    background-color: black;
    color: #fff;
    font-size: var(--primary-font-family);
    font-size: var(--mini);
}

.cart-count-bubble.is-hidden {
    opacity: 0;
}


.header__icon--cart--text {
    display: flex;
    align-items: center;
}


.header__icon--cart--text .cart-count-bubble {
    height: 1.25rem;
    width: 1.25rem;
    font-size: var(--micro);
    line-height: 1;
    right: -.5rem;
    padding-top: .2rem;
    bottom: 0;
    top: 0;
    margin-right: 0;
}

