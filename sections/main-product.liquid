<link rel="preload" href="{{ 'vendor-swiper.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<link rel="preload" href="{{ 'component-accordion.css' | asset_url  }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<link rel="preload" href="{{ 'component-breadcrumbs.css' | asset_url  }}" as="style" onload="this.onload=null;this.rel='stylesheet'">

<noscript><link rel="stylesheet" href="{{ 'vendor-swiper.css' | asset_url }}"></noscript>
